{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "member-access": false,
    "no-null-keyword": false,
    "no-console": false,
    "curly": false,
    "semicolon": [true, "never"],
    "quotemark": [true, "single", "jsx-double"],
    "interface-name": false
  },
  "linterOptions": {
    "exclude": [
      "**/node_modules/**",
      "**/build/**",
      "**/dist/**",
      "**/types/**",
      "**/.next/**"
    ]
  }
}